{
	"default": [
		{
			"type": "insecureAcceptAnything"
		}
	],
	"transports": {
		"docker": {
			"docker.io/dirk1980": [
				{
					"type": "sigstoreSigned",
					"keyPaths": [
						"/usr/share/containers/keys/dirk1980.pub",
						"/usr/share/containers/keys/dirk1980-backup.pub"
					]
				}
			],
			"docker.io/glockgmbh": [
				{
					"type": "sigstoreSigned",
					"keyPaths": [
						"/usr/share/containers/keys/glockgmbh.pub",
						"/usr/share/containers/keys/glockgmbh-backup.pub",
						"/usr/share/containers/keys/dirk1980.pub",
						"/usr/share/containers/keys/dirk1980-backup.pub"
					]
				}
			]
		}
	}
}