{
	"default": [
		{
			"type": "insecureAcceptAnything"
		}
	],
	"transports": {
		"docker": {
			"docker.io": {
				"dirk1980": [
					{
						"type": "sigstoreSigned",
						"keyPaths": [
							"/usr/share/containers/dirk1980.pub",
							"/usr/share/containers/dirk1980-backup.pub"
						],
						"signedIdentity": {
							"type": "matchRepoDigestOrExact"
						}
					}
				],
				"glockgmbh": [
					{
						"type": "sigstoreSigned",
						"keyPaths": [
							"/usr/share/containers/glockgmbh.pub",
							"/usr/share/containers/glockgmbh-backup.pub"
						],
						"signedIdentity": {
							"type": "matchRepoDigestOrExact"
						}
					}
				]
			}
		}
	}
}